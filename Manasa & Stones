#include <iostream>
#include <set>

using namespace std;

int main() {
    int t;  // Number of test cases
    cin >> t;

    while (t--) {
        int n, a, b;
        cin >> n >> a >> b;

        set<int> positions;  // Use set to avoid duplicates and keep sorted order

        for (int i = 0; i < n; ++i) {
            int position = i * a + (n - 1 - i) * b;
            positions.insert(position);
        }

        // Print all possible positions in ascending order
        for (auto pos : positions) {
            cout << pos << " ";
        }
        cout << "\n";
    }

    return 0;
}
